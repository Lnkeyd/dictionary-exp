{
    "rewrites": [
        {
          "source": "/api/login",
          "destination": "https://deploy-dict-exp.vercel.app/api/login"
        },
        {
          "source": "/api/login/",
          "destination": "https://deploy-dict-exp.vercel.app/api/login/"
        },
        {
            "source": "/api/login/logout",
            "destination": "https://deploy-dict-exp.vercel.app/api/login/logout"
          },
        {
            "source": "/api/login/logout/",
            "destination": "https://deploy-dict-exp.vercel.app/api/login/logout/"
          },
        {
            "source": "/api/dict",
            "destination": "https://deploy-dict-exp.vercel.app/api/dict"
        },
        {
          "source": "/api/dict/:path*",
          "destination": "https://deploy-dict-exp.vercel.app/api/dict/:path*"
        },
        {
            "source": "/api/stat",
            "destination": "https://deploy-dict-exp.vercel.app/api/stat"
        },
        {
            "source": "/api/stat/",
            "destination": "https://deploy-dict-exp.vercel.app/api/stat/"
        },
        {
          "source": "/api/check-auth",
          "destination": "https://deploy-dict-exp.vercel.app/api/check-auth"
        },
        {
          "source": "/api/check-auth/",
          "destination": "https://deploy-dict-exp.vercel.app/api/check-auth/"
        },
        {
          "source": "/(.*)",
          "destination": "/"
        }
      ]
}